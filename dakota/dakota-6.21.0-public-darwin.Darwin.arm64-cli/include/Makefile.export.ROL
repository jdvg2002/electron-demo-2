ROL_C_FLAGS= -w 
ROL_C_COMPILER_FLAGS= -w 

ROL_FORTRAN_FLAGS= 
ROL_Fortran_COMPILER_FLAGS= 

## Extra link flags (e.g., specification of fortran libraries)
