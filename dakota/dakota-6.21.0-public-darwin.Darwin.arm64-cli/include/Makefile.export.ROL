ROL_SHARED_LIB_RPATH_COMMAND=-Wl,-rpath,/usr/local/lib
ROL_BUILD_SHARED_LIBS=TRUE

ROL_LINKER=/Library/Developer/CommandLineTools/usr/bin/ld
ROL_AR=/Library/Developer/CommandLineTools/usr/bin/ar

## ---------------------------------------------------------------------------
